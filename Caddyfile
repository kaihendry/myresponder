d.uptown:80 {
	tls hendry@iki.fi
	root docs
	markdown
}

m.uptown:80 {
	tls hendry@iki.fi
	root m
	fastcgi / 127.0.0.1:9000 php
	log logs/m.duroa.access.log
	errors logs/m.duroa.error.log
}

